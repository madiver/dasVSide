{
  "name": "das-hotkey-tools",
  "publisher": "das-hotkey-tools",
  "displayName": "DAS Hotkey Tools",
  "description": "Phase 0 scaffold for DAS Hotkey Tools",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/mark/dasVSide.git"
  },
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dasHotkeyTools.placeholderCommand"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dasHotkeyTools.placeholderCommand",
        "title": "DAS Hotkey Tools: Placeholder Command"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/vsce": "^2.32.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
